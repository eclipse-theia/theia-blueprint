    "@theia/preview": "1.14.0",
    "@theia/process": "1.14.0",
    "@theia/property-view": "1.14.0",
    "@theia/scm": "1.14.0",
    "@theia/scm-extra": "1.14.0",
    "@theia/search-in-workspace": "1.14.0",
    "@theia/task": "1.14.0",
    "@theia/terminal": "1.14.0",
    "@theia/timeline": "1.14.0",
    "@theia/typehierarchy": "1.14.0",
