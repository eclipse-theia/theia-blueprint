    "type": "git",
    "url": "git+https://github.com/eclipse-theia/theia-blueprint.git"
  },
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=12.14.1 <13"
  },
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Theia Blueprint"
      }
    },
    "backend": {
      "config": {
        "startupTimeout": -1
      }
    }
  },
